{
  "name": "react-basic-template",
  "version": "1.0.0",
  "description": "A basic tempate I made for React apps.",
  "main": "index.js",
  "scripts": {
    "start": "yarn run dev",
    "dev": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --port 8080",
    "build": "webpack -p && cp src/index.html dist/index.html"
  },
  "keywords": [
    "reactjs",
    "react",
    "webpack",
    "yarn",
    "babel"
  ],
  "author": "Bauke Posthuma <baukeposthuma@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
